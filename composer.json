{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "laravel/framework": "^12.35.1",
        "laravel/tinker": "^2.10.1",
        "livewire/livewire": "^3.6.4",
        "mariosimao/notion-sdk-php": "^1.15",
        "marvinlabs/laravel-discord-logger": "^1.4.3",
        "spatie/laravel-feed": "^4.4.2",
        "symfony/browser-kit": "^7.3.2"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^3.6.0",
        "fakerphp/faker": "^1.24.1",
        "larastan/larastan": "^3.7.2",
        "laravel/pint": "^1.25.1",
        "mockery/mockery": "^1.6.12",
        "nunomaduro/collision": ">=8.8.2",
        "phpunit/phpunit": "^11.5.42",
        "rector/rector": "^2.2.5",
        "spatie/laravel-ignition": "^2.9.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --ansi"
        ],
        "all": [
            "composer dump-autoload",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:models -WR",
            "@rector",
            "@stan",
            "@pint"
        ],
        "pint": [
            "@php ./vendor/bin/pint"
        ],
        "pint:check": [
            "@php ./vendor/bin/pint --test"
        ],
        "stan": [
            "@php ./vendor/bin/phpstan analyse --memory-limit=2G"
        ],
        "rector": [
            "@php ./vendor/bin/rector --no-diffs"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
